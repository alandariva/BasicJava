{
  "name": "basic-java",
  "version": "0.1.0",
  "description": "A Java interpreter written in JavaScript",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "build": "npm run generate-parser && npx webpack --mode=development",
    "build-prod": "npm run generate-parser && npx webpack --mode=production",
    "generate-parser": "antlr4-tool -l js -o src/generated/antlr src/antlr/BasicJava.g4"
  },
  "author": "Alan Dariva de Oliveira",
  "license": "ISC",
  "devDependencies": {
    "@types/antlr4": "^4.7.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "antlr4": "^4.7.2",
    "antlr4-tool": "^1.0.26",
    "antlr4ts": "^0.5.0-alpha.1",
    "dts-gen": "^0.5.7",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "jest": "^23.6.0",
    "npm": "^6.7.0"
  }
}
